<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Slider?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.morpion.SceneController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="14.0" layoutY="29.0" nodeOrientation="INHERIT" pickOnBounds="true" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../../imagesResources/sunEarth.jpg" />
         </image>
      </ImageView>
      <Slider layoutX="14.0" layoutY="562.0" prefHeight="24.0" prefWidth="145.0" />
      <Button layoutX="317.0" layoutY="217.0" mnemonicParsing="false" onAction="#goGameScene" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="150.0" fitWidth="200.0" onDragDetected="#goGameScene" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagesResources/iconSpaceNavet.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="buttonParametre" layoutX="670.0" layoutY="14.0" mnemonicParsing="false" onAction="#goParameterScene" prefHeight="102.0" prefWidth="116.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="66.0" fitWidth="81.0" onDragDetected="#goParameterScene" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagesResources/iconGears.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="quitButton" layoutX="374.0" layoutY="512.0" mnemonicParsing="false" onAction="#closeAppEvent" text="Quitter" />
   </children>
</AnchorPane>
