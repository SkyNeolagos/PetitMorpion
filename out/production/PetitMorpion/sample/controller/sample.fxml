<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane maxHeight="600.0" maxWidth="800.0" minHeight="600.0" minWidth="800.0" prefHeight="600.0" prefWidth="800.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="sample.controller.SceneController">
   <children>
      <ImageView fitHeight="600.0" fitWidth="800.0" layoutX="14.0" layoutY="29.0" nodeOrientation="INHERIT" pickOnBounds="true" AnchorPane.leftAnchor="0.0" AnchorPane.topAnchor="0.0">
         <image>
            <Image url="@../../imagesResources/background/sunEarth.jpg" />
         </image>
      </ImageView>
      <Button layoutX="319.0" layoutY="116.0" mnemonicParsing="false" onAction="#goSetupScene" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="150.0" fitWidth="200.0" onDragDetected="#goSetupScene" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagesResources/iconSpaceNavet.png" />
               </image>
            </ImageView>
         </graphic></Button>
      <Button fx:id="quitButton" layoutX="665.0" layoutY="474.0" mnemonicParsing="false" onAction="#closeAppEvent" prefHeight="90.0" prefWidth="90.0" style="-fx-background-color: transparent;">
         <graphic>
            <ImageView fitHeight="90.0" fitWidth="90.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../../imagesResources/croix.png" />
               </image>
            </ImageView>
         </graphic></Button>
   </children>
</AnchorPane>
